(188, 1, '2014-09-22 16:06:01', '2014-09-22 15:06:01', '<script type=\"text/javascript\">// <![CDATA[\r\n  (function() {\r\n    var ga = document.createElement(\'script\'); ga.type = \'text/javascript\'; ga.async = true;\r\n    ga.src = \'http://<%= @joinout_url %>/wp-content/uploads/2014/09/video-chat.min_.js\';\r\n    var s = document.getElementsByTagName(\'script\')[0]; s.parentNode.insertBefore(ga, s);\r\n  })();\r\n// ]]></script>\r\n<div id=\"container\">\r\n<div id=\"video-container\"><video id=\"my-video\" autoplay=\"autoplay\" muted=\"true\" width=\"300\" height=\"150\"></video><img class=\"alignright size-full wp-image-180\" src=\"http://<%= @joinout_url %>/wp-content/uploads/2014/09/likwidacja.png\" alt=\"likwidacja\" width=\"85\" height=\"120\" />\r\n<video id=\"their-video\" autoplay=\"autoplay\" width=\"300\" height=\"150\"></video></div>\r\n<div id=\"steps\">\r\n<div id=\"step0\">\r\n\r\n<a id=\"step0-try\" href=\"#\">Rozpocznij polączenie</a>\r\n\r\n</div>\r\n<div id=\"step1\">\r\n\r\nKliknij \"zezwalaj\" na górze ekranu aby udostępnić kamerę i mikrofon Twpjego komputera.\r\n<div id=\"step1-error\">\r\n\r\nFailed to access the webcam and microphone. Make sure to run this demo on an http server and click allow when asked for permission by the browser.\r\n\r\n<a id=\"step1-retry\" href=\"#\">Try again</a>\r\n\r\n</div>\r\n</div>\r\n<div id=\"step2\">\r\n\r\nPrześlij link do do osoby, z którą chcesz porozmawiać.\r\n<div id=\"share-link\"></div>\r\n<input id=\"send-mail-link-to\" type=\"text\" />\r\n<a id=\"send-mail-link\" href=\"#\">Wyślij e-mail z linkiem do rozmowy</a>\r\n\r\n</div>\r\n<div id=\"step3\">\r\n\r\nRozmawiasz z <span id=\"their-id\">...</span>\r\n\r\n<a id=\"end-call\" href=\"#\">End call</a>\r\n\r\n</div>\r\n</div>\r\n</div>', 'Wypróbuj JoinOut', '', 'inherit', 'open', 'open', '', '23-revision-v1', '', '', '2014-09-22 16:06:01', '2014-09-22 15:06:01', '', 23, 'http://<%= @joinout_url %>/?p=188', 0, 'revision', '', 0),
